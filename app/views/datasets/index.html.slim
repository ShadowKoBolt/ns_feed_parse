- if @datasets.any?
  .list-group.dataset-results
    - @datasets.each do |dataset|
      a.list-group-item href=dataset_path(area_id: params[:area_id], subject_id: params[:subject_id], dataset_id: dataset.api_code)
        = dataset.name
        span.badge API ID: #{dataset.api_code}
- else
  h2 No Datasets
